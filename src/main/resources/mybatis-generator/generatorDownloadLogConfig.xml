<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="rainbow" targetRuntime="MyBatis3">
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.1.100:3308/cloud_school_dev"
                        userId="root" password="test123">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.uwaytech.downloadLog.domain" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.uwaytech.downloadLog.mybatis" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.uwaytech.downloadLog.dao" targetProject="src/main/java"
                             implementationPackage="true">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="download_log" domainObjectName="DownloadLog" enableCountByExample="true"
        enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true"
        selectByExampleQueryId="true">
            <columnOverride column="content" jdbcType="VARCHAR" />
        </table>
</context>
</generatorConfiguration>
